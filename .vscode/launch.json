    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to QEMU",
            "type": "gdb",
            "request": "attach",
            "executable": "${workspaceRoot}/target/x86_64-nit_os/debug/nit_os",
            "target": "localhost:1234",
            "remote": true,
            "cwd": "${workspaceRoot}",
            "gdbpath": "gdb-multiarch",
            "valuesFormatting": "prettyPrinters"
        }
    ]
}